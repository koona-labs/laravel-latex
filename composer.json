{
    "name": "abiturma/laravel-latex",
    "description": "A LaTeX compiler wrapper for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abiturma\\LaravelLatex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abiturma\\LaravelLatex\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aaron Kunert",
            "email": "aaron.kunert@abiturma.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/config": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/view": "^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "titasgailius/terminal": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abiturma\\LaravelLatex\\LatexServiceProvider"
            ],
            "aliases": {
                "Latex": "Abiturma\\LaravelLatex\\Facades\\Latex"
            }
        }
    }
}
